{
    "name": "lizhineng/bubble",
    "description": "WeChat SDK for Laravel.",
    "type": "library",
    "require": {
        "php": "^8.0",
        "ext-mbstring": "*",
        "ext-openssl": "*",
        "guzzlehttp/guzzle": "^7.0",
        "illuminate/cache": "^9.0",
        "illuminate/notifications": "^9.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "illuminate/container": "^9.0",
        "illuminate/config": "^9.0",
        "mockery/mockery": "^1.5"
    },
    "autoload": {
        "psr-4": {
            "Zhineng\\Bubble\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Zhineng\\Bubble\\Tests\\": "tests"
        }
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Li Zhineng",
            "email": "im@zhineng.li"
        }
    ],
    "extra": {
        "laravel": {
            "providers": [
                "Zhineng\\Bubble\\BubbleServiceProvider"
            ]
        }
    }
}
